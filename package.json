{
  "name": "@openpatch/ui-core",
  "version": "4.2.7",
  "description": "ui core for openpatch",
  "author": "Mike Barkmin",
  "homepage": "https://openpatch.gitlab.io/ui-core",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:openpatch/ui-core.git"
  },
  "keywords": [
    "react-component"
  ],
  "module": "lib/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "lingui": {
    "sourceLocale": "en",
    "localeDir": "<rootDir>/src/locale",
    "compileNamespace": "es",
    "format": "po",
    "srcPathDirs": [
      "<rootDir>/src"
    ]
  },
  "scripts": {
    "build": "yarn clean && yarn locale:clean && yarn locale:compile && cross-env NODE_ENV=production ./node_modules/.bin/babel src --out-dir lib --copy-files",
    "clean": "rm -rf lib",
    "prepublishOnly": "yarn build",
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "lint": "eslint src",
    "test:coverage": "jest --coverage --collectCoverage",
    "test:watch": "jest --watch",
    "prettier": "prettier --single-quote --write src/**/*.js",
    "locale:extract": "lingui extract",
    "locale:clean": "lingui extract --clean",
    "locale:compile": "lingui compile",
    "locale:add": "lingui add-locale",
    "postpublish": "git push --tags"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "highlight.js": "9.15.10",
    "immer": "4.0.0",
    "lodash": "4.17.15",
    "react-ace": "7.0.4",
    "react-color": "2.17.3",
    "react-markdown": "4.2.2",
    "redux-form": "8.2.6",
    "reselect": "4.0.0",
    "xterm": "3.14.5"
  },
  "peerDependencies": {
    "@lingui/macro": "2.x",
    "@lingui/react": "2.x",
    "@material-ui/core": "4.x",
    "@material-ui/lab": "^4.0.0-alpha.13",
    "@reach/router": "1.x",
    "react": "16.x",
    "react-redux": "7.x",
    "redux": "4.x",
    "redux-thunk": "2.x"
  },
  "devDependencies": {
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@lingui/cli": "2.8.3",
    "@lingui/loader": "2.8.3",
    "@lingui/macro": "2.8.3",
    "@lingui/react": "2.8.3",
    "@material-ui/core": "4.4.3",
    "@material-ui/lab": "4.0.0-alpha.27",
    "@reach/router": "1.2.1",
    "@storybook/addon-actions": "5.2.1",
    "@storybook/addon-info": "5.2.1",
    "@storybook/addon-knobs": "5.2.1",
    "@storybook/addon-links": "5.2.1",
    "@storybook/addon-viewport": "5.2.1",
    "@storybook/addons": "5.2.1",
    "@storybook/react": "5.2.1",
    "axios": "0.19.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-macros": "2.6.1",
    "cross-env": "6.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-react": "7.14.3",
    "husky": "2.7.0",
    "jest": "24.9.0",
    "jest-enzyme": "7.1.1",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-test-renderer": "16.9.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "npm run prettier --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/locale/",
      "<rootDir>/fixtures",
      "<rootDir>/proxies"
    ],
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
