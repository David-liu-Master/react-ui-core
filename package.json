{
  "name": "@openpatch/ui-core",
  "version": "4.0.4",
  "description": "ui core for openpatch",
  "author": "Mike Barkmin",
  "homepage": "https://openpatch.gitlab.io/ui-core",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:openpatch/ui-core.git"
  },
  "keywords": [
    "react-component"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "lingui": {
    "sourceLocale": "en",
    "localeDir": "<rootDir>/src/locale",
    "compileNamespace": "es",
    "format": "po",
    "srcPathDirs": [
      "<rootDir>/src"
    ]
  },
  "scripts": {
    "build": "lingui extract --clean && lingui compile && cross-env NODE_ENV=production ./node_modules/.bin/babel src --out-dir lib --copy-files",
    "prepublishOnly": "npm run build",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "lint": "eslint src",
    "test:coverage": "jest --coverage --collectCoverage",
    "test:watch": "jest --watch",
    "prettier": "prettier --single-quote --write src/**/*.js",
    "locale:extract": "lingui extract",
    "locale:clean": "lingui extract --clean",
    "locale:compile": "lingui compile",
    "locale:add": "lingui add-locale",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "highlight.js": "^9.15.8",
    "immer": "^3.0.0",
    "lodash": "^4.17.11",
    "react-ace": "^7.0.1",
    "react-markdown": "^4.0.3",
    "redux-form": "^8.2.3",
    "reselect": "^4.0.0",
    "xterm": "^3.14.2"
  },
  "peerDependencies": {
    "@lingui/macro": "2.x",
    "@lingui/react": "2.x",
    "@material-ui/core": "4.x",
    "@material-ui/lab": "^4.0.0-alpha.13",
    "@reach/router": "1.x",
    "react": "16.x",
    "react-redux": "7.x",
    "redux": "4.x",
    "redux-thunk": "2.x"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@lingui/cli": "2.8.3",
    "@lingui/loader": "2.8.3",
    "@lingui/macro": "2.8.3",
    "@lingui/react": "2.8.3",
    "@material-ui/core": "4.1.1",
    "@material-ui/lab": "4.0.0-alpha.16",
    "@reach/router": "1.2.1",
    "@storybook/addon-actions": "5.1.4",
    "@storybook/addon-info": "5.1.4",
    "@storybook/addon-knobs": "5.1.4",
    "@storybook/addon-links": "5.1.4",
    "@storybook/addon-viewport": "5.1.4",
    "@storybook/addons": "5.1.4",
    "@storybook/react": "5.1.4",
    "axios": "0.19.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "babel-plugin-macros": "2.6.1",
    "cross-env": "5.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-react": "7.13.0",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "jest-enzyme": "7.0.2",
    "lint-staged": "8.2.0",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
    "react-test-renderer": "16.8.6",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,jsx,json,css}": [
        "npm run prettier --single-quote --write",
        "git add"
      ]
    },
    "ignore": [
      "src/locale/**/*"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/locale/",
      "<rootDir>/fixtures",
      "<rootDir>/proxies"
    ],
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
